<%= form_for @user, url: users_path do |f| %>
    <div class="error">
    <%= h(flash[:notice]) %>
    </div>
    <p>
    <%= f.label :ユーザーネーム %>
    <%= f.text_field :name %>
    </p>
    <p>
    <%= f.label :メール%>
    <%= f.text_field :mail %>
    </p>
    <p>
    <%= f.label :住所 %>
    <%= f.text_field :address %>
    </p>
    <p>
    <%= f.label :エリアマスタID %>
    <%= f.text_field :area_master_id %>
    </p>
    <p>
    <%= f.label :パスワード %>
    <%= f.text_field :password %>
    </p>
    <p>
    <%= f.label :パスワード確認 %>
    <%= f.text_field :password_confirmation %>
    </p>
    <%= f.label :admin %>
    <%= f.select :admin,["false", "true"],value:"false" %>
    <p>
    <%= f.submit '登録' %>
    </p>
    <% end %>